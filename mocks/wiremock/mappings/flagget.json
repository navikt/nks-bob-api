{
  "priority": 1,
  "request": {
    "method": "POST",
    "url": "/api/v1/stream/chat",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$.[?(@.question =~ /.*stygg.*/)]"
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "text/event-stream",
      "Cache-Control": "no-cache",
      "Connection": "keep-alive"
    },
    "bodyFileName": "flagget-data.txt",
    "chunkedDribbleDelay": {
      "numberOfChunks": 1,
      "totalDuration": 500
    }
  }
}